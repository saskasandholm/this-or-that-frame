{
  "name": "this-or-that-frame",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "npx kill-port 3000 && node scripts/verify-env.js && next dev",
    "build": "node scripts/setup-env.js && next build",
    "start": "npx kill-port 3000 && node scripts/verify-env.js && next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "seed": "node prisma/seed.js",
    "postinstall": "prisma generate",
    "verify-env": "node scripts/verify-env.js",
    "clean": "rm -rf .next",
    "prepare": "husky install",
    "add-topics": "node scripts/add-sample-topics.js",
    "admin:migrate": "node prisma/migrations/migrate-admin-schema.js",
    "admin:init": "node scripts/init-admin.js",
    "check-deps": "npx depcheck",
    "deps:update": "npx npm-check-updates -u",
    "md:lint": "markdownlint \"**/*.md\" --ignore node_modules",
    "md:fix": "markdownlint \"**/*.md\" --ignore node_modules --fix",
    "docs:consolidate": "node scripts/cleanup/doc-consolidation.js",
    "docs:update-timestamps": "node scripts/cleanup/update-doc-timestamps.js",
    "docs:maintenance": "npm run md:fix && npm run docs:update-timestamps"
  },
  "prisma": {
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
  },
  "dependencies": {
    "@farcaster/auth-client": "^0.3.0",
    "@farcaster/auth-kit": "^0.6.0",
    "@farcaster/frame-sdk": "^0.0.31",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sentry/nextjs": "^9.6.1",
    "@tanstack/react-query": "^5.69.0",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.5.0",
    "next": "15.2.2",
    "next-themes": "^0.4.6",
    "node-fetch": "^2.7.0",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.23.14",
    "wagmi": "^2.14.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.41.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/node": "^20.17.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^15.5.0",
    "lucide-react": "^0.482.0",
    "markdownlint-cli": "^0.44.0",
    "npm-check-updates": "^17.1.16",
    "prettier": "^3.5.3",
    "tailwind-merge": "^3.0.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
