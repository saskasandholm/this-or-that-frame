# Application URL (required)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Analytics ID
NEXT_PUBLIC_ANALYTICS_ID=

# Optional: If using a database for storing notification tokens
DATABASE_URL="file:./dev.db"

# Optional: If integrating with a specific blockchain RPC
NEXT_PUBLIC_RPC_URL=

# Optional: For verifying frame message signatures (if implementing your own verification)
FRAME_PUBLIC_KEY=

# Optional: If you need to use a specific API for integrations
API_KEY=

# Next.js configuration
NEXT_TELEMETRY_DISABLED=1

# Frame configuration
NEXT_PUBLIC_FRAME_IMAGE_URL=http://localhost:3000/api/og
NEXT_PUBLIC_FRAME_POST_URL=http://localhost:3000/api/frame

# Optional integrations
# NEYNAR_API_KEY=key_xyz...
# PINATA_JWT=jwt_xyz... 